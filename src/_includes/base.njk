<!DOCTYPE html>
<html lang="zxx">
  <head>
    <meta charset="utf-8" />
    <title>Entrenets: {{title|safe}}</title>
    <meta name="generator" content="Eleventy" />
    <!--Meta For No Index-->
    <meta name="robots" content="noindex, Nofollow, Noimageindex" />

    <!-- mobile responsive meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, maximum-scale=1"
    />

    <!-- Slick Carousel -->
    <link rel="stylesheet" href="/assets/vendor/slick/slick.css" />
    <link rel="stylesheet" href="/assets/vendor/slick/slick-theme.css" />
    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="/assets/vendor/font-awesome/css/font-awesome.min.css"
    />
    <!-- Bootstrap -->
    <link rel="stylesheet" href="/assets/vendor/bootstrap/bootstrap.min.css" />

    <!-- Stylesheets -->
    <link href="/assets/css/base.module.css" rel="stylesheet" />
    <link href="/assets/css/{{ stylesheet }}" rel="stylesheet" />
    <link href="/assets/css/shared.css" rel="stylesheet" />

    <!--Favicon-->
    <link
      rel="shortcut icon"
      href="/assets/images/favicon.png"
      type="image/x-icon"
    />
    <link rel="icon" href="/assets/images/favicon.png" type="image/x-icon" />
  </head>

  <body>
    {% block navbar %}
    <nav class="navbar navbar-expand-lg site-navigation">
      <div class="container">
        {#
        <h6 class="logo">Entrenets</h6>
        #}

        <a class="navbar-brand" href="/">
          <img src="{{nav.navLogo}}" alt="nav-logo" class="logo" />
        </a>

        <button
          class="navbar-toggler collapsed"
          type="button"
          data-toggle="collapse"
          data-target="#sitenavbar"
        >
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>

        <div class="collapse navbar-collapse" id="sitenavbar">
          <ul class="navbar-nav ml-auto main-nav">
            {% for navLink in nav.navLinks %}
            <li class="nav-item">
              <a class="nav-link" href="{{ navLink.url | default('/') }}"
                >{{navLink.text | capitalize}}</a
              >
              {% if navLink.sublinks %}
              <ul class="submenu">
                {% for sublink in navLink.sublinks %}
                <li class="submenu-list-item">
                  <img src="{{sublink.icon}}" width="20px" />
                  <a href="/{{ sublink.url}}" class="sublink"
                    >{{ sublink.text }}</a
                  >
                </li>
                {% endfor %}
              </ul>
              {% endif %}
            </li>
            {% endfor %}
            <li class="nav-item">
              <a class="nav-link btn btn-sm btn-sm-rounded CTA" href="/{{nav.navBtnLink}}">
                <span class="btn-area">
                  <span data-text="{{nav.navBtnText}}">
                    {{nav.navBtnText}}
                  </span>
                </span>
              </a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
    {% endblock %} {% block content %} {{content | safe}} {% endblock %} {%
    block footer %}
    <footer class="site-footer">
      <div class="container">
        <div class="row">
          <div class="col-lg-3 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">
                {{footer.contactsHeading}}
              </h5>
              <p class="site-footer-widget-description">
                {% for contactInfo in footer.contactsInfo %}
                <a href="{{contactInfo.link}}">{{contactInfo.text}}</a>
                <br />
                {% endfor %}
              </p>
            </div>
          </div>
          <div class="col-lg-2 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">
                {{footer.siteMapHeading}}
              </h5>
              <ul class="site-footer-widget-links">
                {% for siteMap in footer.siteMap %}
                <li>
                  <a href="/{{siteMap.link}}">{{siteMap.text}}</a>
                </li>
                {% endfor %}
              </ul>
            </div>
          </div>
          {# <div class="col-lg-3 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">{{footer.brandHeading}}</h5>
              <p class="site-footer-widget-description">
                {% for brandMotto in footer.brandMotto %} - {{brandMotto}}
                <br />
                {% endfor %}
              </p>
            </div>
          </div> #}
          <div class="col-lg-2 col-12">
            <a href="#top" class="site-footer-widget-top">
              <img src="{{footer.back2topIcon}}" alt="back-to-top" />
              <p>{{footer.customerComment}}</p>
            </a>
          </div>
          <div class="col-12">
            <div class="site-footer-copyright text-center my-3">
              <p>Â© 2025 Entrenets</p>
            </div>
          </div>
        </div>
      </div>
    </footer>
    {% endblock %}
    <!-- <script src="js/vendor.js"></script> -->
    <script src="/assets/vendor/jQuery/jquery.min.js"></script>
    <script src="/assets/vendor/bootstrap/bootstrap.min.js"></script>
    <script src="/assets/js/script.js"></script>
  </body>
</html>
